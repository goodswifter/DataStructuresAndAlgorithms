package com.ad;

/**
 * @author jingshi
 *
 */
public class Main {
	
	public static void main(String[] args) {
		fib1(20);
	}


	/*
	 * 菲波那切数列 : fibonacci number
	 * 0 1 1 2 3 5 8 13 ...
	 */
	public static int fib1(int n) {
		if (n < 0) return 0;
		
		if (n <= 1) 
			return n;
		
		return fib1(n - 1) + fib1(n - 2);
	}
		

}
